{
  "version": 2,
  "public": true,
  "framework": "nextjs",
  "buildCommand": "next build",
  "installCommand": "npm install",
  "regions": ["sfo1"],
  "rewrites": [
    { "source": "/", "destination": "/waitlist" }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/static/(.*)", "dest": "/static/$1" }
  ],
  "github": {
    "silent": true
  }
} 